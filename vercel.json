{
  "rewrites": [
    { "source": "/login", "destination": "/auth.html" },
    { "source": "/register", "destination": "/register.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    
    { "source": "/", "destination": "/index.html" },
    
    { "source": "/:username", "destination": "/profile.html?u=:username" }
  ]
}